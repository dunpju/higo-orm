    return him.DefaultConnect
}

func (this *Model) TableName() *arm.TableName {
    return TableName()
}

func (this *Model) Apply(model *arm.Model) {
    this.Model = model
}

func (this *Model) Exist() bool {
    return this.%PRIMARY_KEY% > 0
}

